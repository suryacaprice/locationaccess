{
  "name": "uk.co.workingedge.phonegap.plugins.example.requestlocationaccuracy",
  "version": "2.1.0",
  "displayName": "Request Location Accuracy example",
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova.plugins.diagnostic": {
        "ANDROID_SUPPORT_VERSION": "28.+"
